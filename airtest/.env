# ==========================================

# === LLM服务配置 ===
# OpenAI Compatible API配置
LLM_PROVIDER=openai_compatible
LLM_API_URL=https://api.llmproai.xyz/v1
LLM_API_KEY=sk-we2QfYIFOM8mgummIvynX1OD7lTlaD3g2m5xw0P1OmgSGrXT
LLM_MODEL=gpt-oss-120b
LLM_TEMPERATURE=0.7
LLM_TIMEOUT=30

# 备用LLM配置 (如果主服务不可用)
LLM_FALLBACK_PROVIDER=openai
LLM_FALLBACK_API_KEY=your-openai-api-key-here
LLM_FALLBACK_MODEL=gpt-3.5-turbo

# === Omniparser视觉识别服务 ===
OMNIPARSER_SERVER_URL=http://100.122.57.128:9333
OMNIPARSER_API_KEY=your-omniparser-key
OMNIPARSER_TIMEOUT=15

# === 数据库配置 (如果需要) ===
# 用于存储测试历史、设备信息等
DATABASE_URL=sqlite:///only_test.db
# DATABASE_URL=postgresql://user:pass@localhost/only_test

# === 外部服务集成 ===
# Allure TestOps (如果使用)
ALLURE_SERVER_URL=http://your-allure-server
ALLURE_PROJECT_ID=your-project-id
ALLURE_TOKEN=your-allure-token

# Jenkins集成 (如果使用)
JENKINS_URL=http://your-jenkins-server
JENKINS_USER=your-jenkins-user
JENKINS_TOKEN=your-jenkins-token

SLACK_WEBHOOK=https://hooks.slack.com/services/xxx
SLACK_CHANNEL=#testing

# === 开发环境配置 ===
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=DEBUG

# === 安全配置 ===
# API访问令牌 (用于REST API)
API_SECRET_KEY=your-secret-key-for-api-access
JWT_SECRET=your-jwt-secret-for-token-auth

# === 缓存配置 ===
REDIS_URL=redis://localhost:6379/0
CACHE_TTL=3600

